<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ltns.rest_area.domain.admin.ScheduleDAO">
	
	<select id="select"  flushCache="true"  resultType="com.ltns.rest_area.domain.admin.ScheduleDTO">
			SELECT
					subject,startdate,enddate,memo
			FROM 
					schedule
	</select>
	
	<insert id="inset_"  flushCache="true"  parameterType="com.ltns.rest_area.domain.admin.ScheduleDTO">
		INSERT INTO
			schedule(s_sid, subject, startDate, endDate, memo)
		VALUES
			(s_sid_seq.nextval, #{param1}, #{param2}, #{param3}, #{param4})
	</insert>
	
	
	<delete id="deleteByName"   flushCache="true" parameterType="com.ltns.rest_area.domain.admin.ScheduleDTO">
		Delete from schedule where subject = #{param1}
	
	</delete>
	
	
	
	<update id="update_" flushCache="true" parameterType="com.ltns.rest_area.domain.admin.ScheduleDTO">
			UPDATE SCHEDULE SET 
				subject= #{param2}, startDate=#{param3}, endDate= #{param4}, memo= #{param5}
			WHERE subject=#{param1}
	</update>

</mapper>


